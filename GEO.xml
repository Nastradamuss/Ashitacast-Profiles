		<var name="Impact">0</var>
	</variables>
	<inputcommands>
		<cmd input="/killall">
			<setvar name="Set" value="No" />
			<setvar name="IdleSet" value="TP" />
			<setvar name="NukeSet" value="Free" />
			<setvar name="EnfeebleSet" value="Low" />
			<setvar name="MDTSetActive" value="0" />
			<setvar name="PDTSetActive" value="0" />
			<setvar name="TH" value="0" />
			<setvar name="Reraise" value="0" />
			<setvar name="Refresh" value="0" />
			<setvar name="AF" value="0" />
			<setvar name="Impact" value="0" />
			<addtochat color="36">KILLED IT ALL, son!</addtochat>
		</cmd>

		<cmd input="/report">
						<addtochat color="36">Idle: [$IdleSet], Nuke: [$NukeSet], Enfeeble: [$EnfeebleSet]</addtochat>
		</cmd>
		<cmd input="/acc">

			<if advanced="$Set=No">
				<setvar name="Set" value="Melee" />
				<addtochat color="36">Melee mode, son!</addtochat>
			</if>
			<else>
				<setvar name="Set" value="No" />
				<addtochat color="36">Let's just cast some shit, son!</addtochat>
			</else>
		</cmd>
		<cmd input="/idleset ">
				         <setvar name="IdleSet" value="%arg1" />
            <addtochat color="159">Idle set in use: $IdleSet</addtochat>
		</cmd>
		<cmd input="/nukeset ">
		            <setvar name="NukeSet" value="%arg1" />
            <addtochat color="159">Nuke set in use: $NukeSet</addtochat>
		</cmd>
		<cmd input="/enfeebleset ">
				            <setvar name="EnfeebleSet" value="%arg1" />
            <addtochat color="159">Enfeeble set in use: $EnfeebleSet</addtochat>
		</cmd>
		<cmd input="/acc">

			<if advanced="$Set=No">
				<setvar name="Set" value="Melee" />
				<addtochat color="36">Melee mode, son!</addtochat>
			</if>
			<else>
				<setvar name="Set" value="No" />
				<addtochat color="36">Let's just cast some shit, son!</addtochat>
			</else>
		</cmd>
		<cmd input="/burst">
			<if advanced="$burst=0">
				<setvar name="Burst" value="1" />
				<addtochat color="36">Burstin' makes me feel good!</addtochat>
			</if>
			<else>
				<setvar name="Burst" value="0" />
				<addtochat color="36">Let's Burst some other time...</addtochat>
			</else>
		</cmd>
		<cmd input="/refresh">
			<if advanced="$Refresh=0">
				<setvar name="Refresh" value="1" />
				<addtochat color="36">NEED MOAR MP!</addtochat>
			</if>
			<else>
				<setvar name="Refresh" value="0" />
				<addtochat color="36">Let's use that MP!</addtochat>
			</else>
		</cmd>
		<cmd input="/th">
			<if advanced="$TH=0">
				<setvar name="TH" value="1" />
				<addtochat color="36">Treasure Hunter TP set enabled!</addtochat>
			</if>
			<else>
				<setvar name="TH" value="0" />
				<addtochat color="36">Treasure Hunter set TP disabled!</addtochat>
			</else>
		</cmd>	

		<!-- MDT set toggle -->
		<!-- As stated, you can alter the command input below if you wish -->
		<cmd input="/mdt">

			<setvar name="PDTSetActive" value="0" />
			<if advanced="$MDTSetActive=0">
				<setvar name="MDTSetActive" value="1" />
				<addtochat color="36">MDT set enabled!</addtochat>
			</if>
			<else>
				<setvar name="MDTSetActive" value="0" />
				<addtochat color="36">MDT set disabled.</addtochat>
			</else>
		</cmd>
		<!-- PDT set toggle -->
		<!-- As stated, you can alter the command input below if you wish -->
		<cmd input="/pdt">

			<setvar name="MDTSetActive" value="0" />
			<if advanced="$PDTSetActive=0">
				<setvar name="PDTSetActive" value="1" />
				<addtochat color="36">PDT set enabled!</addtochat>
			</if>
			<else>
				<setvar name="PDTSetActive" value="0" />
				<addtochat color="36">PDT set disabled.</addtochat>
			</else>
		</cmd>
	</inputcommands>



