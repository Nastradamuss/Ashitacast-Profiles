					<equip set="Enfeebles"/>
			</elseif>      
		</if>
		<elseif ad_type="blackmagic">
			<!--<addtochat color="36">Mid Black Magic</addtochat>       -->  
			<if ad_skill="elementalmagic">
				<addtochat color="36">Mid Black Elemental Magic</addtochat>

				<if ad_name="Impact"><equip set="Impact" lock="true" /></if>	
				<elseif ad_name="*helix">
					<equip set="Helix" />
				</elseif>
			<elseif>
                <equip when="$GearTime" set="Nuke-$NukeSet" />
