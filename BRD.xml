		<var name="MDTSetActive">0</var>
		<var name="PDTSetActive">0</var>
		<var name="TH">0</var>
		<var name="StringVar">false</var>
	</variables>
	<inputcommands>
		<cmd input="/killall">
			<setvar name="Set" value="No" />
			<setvar name="IdleSet" value="Refresh" />
			<setvar name="EnfeebleSet" value="Low" />		
			<setvar name="Set" value="No" />
			<setvar name="MDTSetActive" value="0" />
			<setvar name="PDTSetActive" value="0" />
			<setvar name="TH" value="0" />
			<setvar name="Reraise" value="0" />
			<addtochat color="57">KILLED IT ALL, son!</addtochat>
		</cmd>
		
		
		<cmd input="/singstring ">
			<setvar name="StringVar" value="true" />
			<command>/ma %args</command>		
		</cmd>
		<cmd input="/stringonce">
			<setvar name="StringVar" value="true" />
			<addtochat color="159">Next song will use Terpander.</addtochat>
		</cmd>
		<cmd input="/report">
						<addtochat color="57">Idle: [$IdleSet], Nuke: [$NukeSet], Enfeeble: [$EnfeebleSet]</addtochat>
		</cmd>
		<cmd input="/acc">

			<if advanced="$Set=No">
				<setvar name="Set" value="Melee" />
				<addtochat color="57">Melee mode, son!</addtochat>
			</if>
			<else>
				<setvar name="Set" value="No" />
				<addtochat color="57">Let's just cast some shit, son!</addtochat>
			</else>
		</cmd>
		<cmd input="/idleset ">
				         <setvar name="IdleSet" value="%arg1" />
            <addtochat color="159">Idle set in use: $IdleSet</addtochat>
		</cmd>
		<cmd input="/nukeset ">
		            <setvar name="NukeSet" value="%arg1" />
            <addtochat color="159">Nuke set in use: $NukeSet</addtochat>
		</cmd>
		<cmd input="/enfeebleset ">
				            <setvar name="EnfeebleSet" value="%arg1" />
            <addtochat color="159">Enfeeble set in use: $EnfeebleSet</addtochat>
		</cmd>

		<!-- MDT set toggle -->
		<!-- As stated, you can alter the command input below if you wish -->
		<cmd input="/mdt">

			<setvar name="PDTSetActive" value="0" />
			<if advanced="$MDTSetActive=0">
				<setvar name="MDTSetActive" value="1" />
				<addtochat color="57">MDT set enabled!</addtochat>
			</if>
			<else>
				<setvar name="MDTSetActive" value="0" />
				<addtochat color="57">MDT set disabled.</addtochat>
			</else>
		</cmd>
		<!-- PDT set toggle -->
		<!-- As stated, you can alter the command input below if you wish -->
		<cmd input="/pdt">

			<setvar name="MDTSetActive" value="0" />
			<if advanced="$PDTSetActive=0">
				<setvar name="PDTSetActive" value="1" />
				<addtochat color="57">PDT set enabled!</addtochat>
			</if>
			<else>
				<setvar name="PDTSetActive" value="0" />
				<addtochat color="57">PDT set disabled.</addtochat>
			</else>
		</cmd>
	</inputcommands>



