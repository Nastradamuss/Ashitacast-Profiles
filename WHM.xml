				</if>
			</if>
			<elseif ad_skill="divinemagic">
				<!--<addtochat color="36">Pre Divine Magic</addtochat>-->
			</elseif>
			<elseif ad_skill="enhancingmagic">
				<!--<addtochat color="36">Pre White Enhancing Magic</addtochat>-->
			</elseif>
			<elseif ad_skill="enfeeblingmagic">
