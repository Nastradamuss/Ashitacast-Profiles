	<variables>
		<var name="init">0</var>
		<var name="Set">No</var>
		<var name="IdleSet">TP</var>
		<var name="NukeSet">Free</var>
		<var name="EnfeebleSet">Low</var>
		<var name="MDTSetActive">0</var>
		<var name="PDTSetActive">0</var>
		<var name="TH">0</var>
		<var name="Burst">0</var>
		<var name="Refresh">0</var>
		<var name="AF">0</var>
		<var name="Impact">0</var>
	</variables>
	<inputcommands>
		<!-- Resets All Toggles to Default! -->
		<cmd input="/killall">
			<setvar name="Set" value="No" />
			<setvar name="IdleSet" value="TP" />
			<setvar name="NukeSet" value="Free" />
			<setvar name="EnfeebleSet" value="Low" />
			<setvar name="MDTSetActive" value="0" />
			<setvar name="PDTSetActive" value="0" />
			<setvar name="TH" value="0" />
			<setvar name="Reraise" value="0" />
			<setvar name="Refresh" value="0" />
			<setvar name="AF" value="0" />
			<setvar name="Impact" value="0" />
			<addtochat color="57">KILLED IT ALL, son!</addtochat>
		</cmd>

		<!-- Report macro to see what your current settings are -->
		<cmd input="/report">
						<addtochat color="57">Idle: [$IdleSet], Nuke: [$NukeSet], Enfeeble: [$EnfeebleSet]</addtochat>
		</cmd>
		
		<!-- To swap melee settings -->
		<cmd input="/acc">

			<if advanced="$Set=No">
				<setvar name="Set" value="Melee" />
				<addtochat color="57">Melee mode, son!</addtochat>
			</if>
			<else>
				<setvar name="Set" value="No" />
				<addtochat color="57">Let's just cast some shit, son!</addtochat>
			</else>
		</cmd>
		
		<!-- Use "/idleset Refresh", "/idleset DT", etc in macros -->
		<cmd input="/idleset ">
				         <setvar name="IdleSet" value="%arg1" />
            <addtochat color="159">Idle set in use: $IdleSet</addtochat>
		</cmd>
		
		<!-- Use "/nukeset Free", "/nukeset AF," etc in macros -->
		<cmd input="/nukeset ">
		            <setvar name="NukeSet" value="%arg1" />
            <addtochat color="159">Nuke set in use: $NukeSet</addtochat>
		</cmd>
		
		<!-- Use "/enfeebleset Max" or "/enfeebleset Low" in macro -->
		<cmd input="/enfeebleset ">
				            <setvar name="EnfeebleSet" value="%arg1" />
            <addtochat color="159">Enfeeble set in use: $EnfeebleSet</addtochat>
		</cmd>
		
		
		<!-- Impact toggle macro -->
		<cmd input="/impact">
			<if advanced="$Impact=0">
				<setvar name="Impact" value="1" />
				<addtochat color="57">Impact on</addtochat>
			</if>
			<else>
				<setvar name="Impact" value="0" />
				<addtochat color="57">Impact off</addtochat>
			</else>
		</cmd>				

		<!-- MB toggle macro -->
		<cmd input="/burst">
