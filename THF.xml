	<!-- Don't fiddle with these unless you know how it works -->
	<!-- This is how I've set up the option to have various gear sets
                                     via input commands such as /th or /eva. They all work, however
                                         they may not be coded 'optimally' so if you can clean it up, feel free -->
	<variables>
		<var name="TH" value="0" />
		<var name="Set">LowAcc</var>
		<var name="EvasionSetActive">0</var>
		<var name="MDTSetActive">0</var>
		<var name="PDTSetActive">0</var>
		<var name="TH">0</var>
	</variables>

	<inputcommands>
		<cmd input="/killall">


			<setvar name="MDTSetActive" value="0" />
			<setvar name="PDTSetActive" value="0" />
			<setvar name="EvasionSetActive" value="0" />
			<setvar name="TH" value="0" />

			<addtochat color="57">KILLED IT ALL, son!</addtochat>
		</cmd>
		<cmd input="/acc">

			<if advanced="$Set=LowAcc">
				<setvar name="Set" value="MidAcc" />
				<addtochat color="57">Mid Accuacy</addtochat>
			</if>
			<elseif advanced="$Set=MidAcc">
				<setvar name="Set" value="HighAcc" />
				<addtochat color="57">High accuracy</addtochat>
			</elseif>
			<elseif>
				<setvar name="Set" value="LowAcc" />
				<addtochat color="57">Low accuracy</addtochat>
			</elseif>
		</cmd>

		<!-- Treasure Hunter Toggle (If you want to replace the /th into
                                                 something more user friendly, you can alter it just below -->
		<cmd input="/th">
			<if advanced="$TH=0">
				<setvar name="TH" value="1" />
				<addtochat color="57">Treasure Hunter TP set enabled!</addtochat>
			</if>
			<else>
				<setvar name="TH" value="0" />
				<addtochat color="57">Treasure Hunter set TP disabled!</addtochat>
			</else>
		</cmd>

		<!-- Evasion set toggle -->
		<!-- As stated, you can alter the command input below if you wish -->
		<cmd input="/eva">
			<setvar name="MDTSetActive" value="0" />
			<setvar name="PDTSetActive" value="0" />
			<if advanced="$EvasionSetActive=0">
				<setvar name="EvasionSetActive" value="1" />
