	<!-- Don't fiddle with these unless you know how it works -->
	<!-- This is how I've set up the option to have various gear sets
                                     via input commands such as /th or /eva. They all work, however
                                         they may not be coded 'optimally' so if you can clean it up, feel free -->
	<variables>
		<var name="MP">0</var>
		<var name="CP">0</var>	
		<var name="hybrid">Off</var>
		<var name="Set">Old</var>
		<var name="MDTSetActive">0</var>
		<var name="PDTSetActive">0</var>
		<var name="TH">0</var>
		<var name="Reraise">0</var>
		<var name="Death">0</var>
		<var name="Charm">0</var>
		<var name="SIR">0</var>	
		<var name="MagicEva">0</var>		
	</variables>

	<inputcommands>
		<cmd input="/killall">
			<setvar name="MP" value="0" />
			<setvar name="CP" value="0" />
			<setvar name="SIR" value="0" />
			<setvar name="Charm" value="0" />
			<setvar name="Death" value="0" />
			<setvar name="MagicEva" value="0" />			
			<setvar name="hybrid" value="Off" />
			<setvar name="MDTSetActive" value="0" />
			<setvar name="PDTSetActive" value="0" />
			<setvar name="TH" value="0" />
			<setvar name="Reraise" value="0" />
			<addtochat color="36">[Everything has been reset!]</addtochat>
		</cmd>
	<cmd input="/cp">
			<if advanced="$CP=0">
				<setvar name="CP" value="1" />
				<addtochat color="36">[CP Cape:] On</addtochat>
			</if>
			<else>
				<setvar name="CP" value="0" />
				<addtochat color="36">[CP Cape:] Off</addtochat>
			</else>
	</cmd>			
		<cmd input="/mp">
			<if advanced="$MP=0">
				<setvar name="MP" value="1" />
				<addtochat color="36">[Refresh:] On</addtochat>
			</if>
			<else>
				<setvar name="MP" value="0" />
				<addtochat color="36">[Refresh:] Off</addtochat>
			</else>		
		</cmd>		
		<cmd input="/acc">

			<if advanced="$Set=Old">
				<setvar name="Set" value="Acc" />
				<addtochat color="36">[Accuracy:] On</addtochat>
			</if>
			<else>
				<setvar name="Set" value="Old" />
				<addtochat color="36">[Accuracy:] Off</addtochat>
			</else>
		</cmd>
	<cmd input="/sir">
			<if advanced="$SIR=0">
				<setvar name="SIR" value="1" />
				<addtochat color="36">[Spell interrupt:] On</addtochat>
			</if>
			<else>
